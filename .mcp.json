{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": ""
      }
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-exa"
      ],
      "env": {
        "EXA_API_KEY": ""
      }
    },
    "firecrawl": {
      "command": "npx",
      "args": [
        "-y",
        "@mendable/firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": ""
      }
    },
    "conversation-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-claude-conversation-search"
      ]
    },
    "vercel": {
      "command": "npx",
      "args": [
        "-y",
        "@vercel/mcp-server"
      ],
      "env": {
        "VERCEL_ACCESS_TOKEN": ""
      }
    },
    "stripe": {
      "command": "npx",
      "args": [
        "-y",
        "@stripe/mcp-server"
      ],
      "env": {
        "STRIPE_API_KEY": ""
      }
    }
  }
}
